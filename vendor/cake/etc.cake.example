CAKE_DEFAULT_COMPILER_PREFIX="GCC"
CAKE_DEFAULT_VARIANT="${CAKE_DEFAULT_COMPILER_PREFIX}_debug
CAKE_PROJECT_VERSION_CMD="echo 0.0.0-1"
CAKE_PREPROCESS=False

CAKE_GCC_ID="GCC"
CAKE_GCC_CPP="ccache g++"
CAKE_GCC_CC="ccache gcc"
CAKE_GCC_CXX="ccache g++"
CAKE_GCC_LINKER="g++"
CAKE_GCC_CPPFLAGS=" -I . "
CAKE_GCC_CFLAGS=" -I . -fPIC -g -Wall"
CAKE_GCC_CXXFLAGS=" -I . -fPIC -g -Wall"
CAKE_GCC_LINKFLAGS="-fPIC -Wall -Werror -Xlinker --build-id"
CAKE_GCC_TESTPREFIX="timeout 300 valgrind --quiet --error-exitcode=1"
CAKE_GCC_POSTPREFIX="timeout 120"

CAKE_GCC_DEBUG_ID="$CAKE_GCC_ID"
CAKE_GCC_DEBUG_CPP="$CAKE_GCC_CPP"
CAKE_GCC_DEBUG_CC="$CAKE_GCC_CC"
CAKE_GCC_DEBUG_CXX="$CAKE_GCC_CXX"
CAKE_GCC_DEBUG_LINKER="$CAKE_GCC_LINKER"
CAKE_GCC_DEBUG_CPPFLAGS="${CAKE_GCC_CPPFLAGS}"
CAKE_GCC_DEBUG_CFLAGS="$CAKE_GCC_CFLAGS"
CAKE_GCC_DEBUG_CXXFLAGS="${CAKE_GCC_CXXFLAGS}"
CAKE_GCC_DEBUG_LINKFLAGS="$CAKE_GCC_LINKFLAGS"
CAKE_GCC_DEBUG_TESTPREFIX="valgrind --quiet --error-exitcode=1"
CAKE_GCC_DEBUG_POSTPREFIX="timeout 120"

CAKE_GCC_RELEASE_ID="$CAKE_GCC_ID"
CAKE_GCC_RELEASE_CPP="$CAKE_GCC_CPP"
CAKE_GCC_RELEASE_CC="$CAKE_GCC_CC"
CAKE_GCC_RELEASE_CXX="$CAKE_GCC_CXX"
CAKE_GCC_RELEASE_LINKER="$CAKE_GCC_LINKER"
CAKE_GCC_RELEASE_CPPFLAGS="${CAKE_GCC_CPPFLAGS}"
CAKE_GCC_RELEASE_CFLAGS="$CAKE_GCC_CFLAGS -fPIC -O3 -DNDEBUG -Wall -finline-functions -Wno-inline"
CAKE_GCC_RELEASE_CXXFLAGS="${CAKE_GCC_CXXFLAGS} -fPIC -O3 -DNDEBUG -Wall -finline-functions -Wno-inline"
CAKE_GCC_RELEASE_LINKFLAGS="-O3 -Wall -Xlinker --build-id"
CAKE_GCC_RELEASE_TESTPREFIX=""
CAKE_GCC_RELEASE_POSTPREFIX="timeout 120"

CAKE_GCC_PROFILE_ID="$CAKE_GCC_ID"
CAKE_GCC_PROFILE_CPP="$CAKE_GCC_CPP"
CAKE_GCC_PROFILE_CC="$CAKE_GCC_CC"
CAKE_GCC_PROFILE_CXX="$CAKE_GCC_CXX"
CAKE_GCC_PROFILE_LINKER="$CAKE_GCC_LINKER"
CAKE_GCC_PROFILE_CPPFLAGS="$CAKE_GCC_RELEASE_CPPFLAGS"
CAKE_GCC_PROFILE_CFLAGS="$CAKE_GCC_RELEASE_CFLAGS -pg -g"
CAKE_GCC_PROFILE_CXXFLAGS="$CAKE_GCC_RELEASE_CXXFLAGS -pg -g"
CAKE_GCC_PROFILE_LINKFLAGS="-O3 -Wall -pg -g"
CAKE_GCC_PROFILE_TESTPREFIX=""
CAKE_GCC_PROFILE_POSTPREFIX="timeout 120"

CAKE_GCC_COVERAGE_ID="$CAKE_GCC_ID"
CAKE_GCC_COVERAGE_CPP="$CAKE_GCC_CPP"
CAKE_GCC_COVERAGE_CC="$CAKE_GCC_CC"
CAKE_GCC_COVERAGE_CXX="$CAKE_GCC_CXX"
CAKE_GCC_COVERAGE_LINKER="$CAKE_GCC_LINKER"
CAKE_GCC_COVERAGE_CPPFLAGS="${CAKE_GCC_CPPFLAGS}"
CAKE_GCC_COVERAGE_CFLAGS="$CAKE_GCC_CFLAGS -fPIC -O0 -fno-inline -Wall -g -fprofile-arcs -ftest-coverage"
CAKE_GCC_COVERAGE_CXXFLAGS="${CAKE_GCC_CXXFLAGS} -fPIC -O0 -fno-inline -Wall -g -fprofile-arcs -ftest-coverage"
CAKE_GCC_COVERAGE_LINKFLAGS="-fPIC -O0 -fno-inline -Wall -g -fprofile-arcs -ftest-coverage"
CAKE_GCC_COVERAGE_TESTPREFIX="valgrind --quiet --error-exitcode=1"
CAKE_GCC_COVERAGE_POSTPREFIX="timeout 120"

CAKE_CLANG_ID="CLANG"
CAKE_CLANG_CPP="ccache g++"
CAKE_CLANG_CC="ccache clang "
CAKE_CLANG_CXX="ccache clang++"
CAKE_CLANG_LINKER="clang++"
CAKE_CLANG_CPPFLAGS=" -I . "
CAKE_CLANG_CFLAGS=" -I . -fPIC -g -Wall"
CAKE_CLANG_CXXFLAGS=" -I . -fPIC -g -Wall "
CAKE_CLANG_LINKFLAGS="-fPIC -Wall -Werror -Xlinker --build-id"
CAKE_CLANG_TESTPREFIX="timeout 300 valgrind --quiet --error-exitcode=1"
CAKE_CLANG_POSTPREFIX="timeout 120"

CAKE_CLANG_DEBUG_ID="$CAKE_CLANG_ID"
CAKE_CLANG_DEBUG_CPP="$CAKE_CLANG_CPP"
CAKE_CLANG_DEBUG_CC="$CAKE_CLANG_CC"
CAKE_CLANG_DEBUG_CXX="$CAKE_CLANG_CXX"
CAKE_CLANG_DEBUG_LINKER="$CAKE_CLANG_LINKER"
CAKE_CLANG_DEBUG_CPPFLAGS="$CAKE_CLANG_CPPFLAGS"
CAKE_CLANG_DEBUG_CFLAGS="$CAKE_CLANG_CFLAGS"
CAKE_CLANG_DEBUG_CXXFLAGS="$CAKE_CLANG_CXXFLAGS"
CAKE_CLANG_DEBUG_LINKFLAGS="$CAKE_CLANG_LINKFLAGS"
CAKE_CLANG_DEBUG_TESTPREFIX="valgrind --quiet --error-exitcode=1"
CAKE_CLANG_DEBUG_POSTPREFIX="timeout 120"

CAKE_CLANG_RELEASE_ID="$CAKE_CLANG_ID"
CAKE_CLANG_RELEASE_CPP="$CAKE_CLANG_CPP"
CAKE_CLANG_RELEASE_CC="$CAKE_CLANG_CC"
CAKE_CLANG_RELEASE_CXX="$CAKE_CLANG_CXX"
CAKE_CLANG_RELEASE_LINKER="$CAKE_CLANG_LINKER"
CAKE_CLANG_RELEASE_CPPFLAGS="$CAKE_CLANG_CPPFLAGS"
CAKE_CLANG_RELEASE_CFLAGS="-fPIC -O3 -DNDEBUG -Wall -finline-functions -Wno-inline"
CAKE_CLANG_RELEASE_CXXFLAGS="-fPIC -O3 -DNDEBUG -Wall -finline-functions -Wno-inline"
CAKE_CLANG_RELEASE_LINKFLAGS="-O3 -Wall -Xlinker --build-id"
CAKE_CLANG_RELEASE_TESTPREFIX=""
CAKE_CLANG_RELEASE_POSTPREFIX="timeout 120"
